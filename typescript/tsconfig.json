{
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "target": "es5",
    "lib": ["ESNext", "ES2015", "DOM"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
